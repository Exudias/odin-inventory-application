<div class="add-view">
    <h1>Update developer: <%=  dev.name %></h1>
    <form action="/developers/<%= id %>?_method=PUT" method="POST">
        <label>
            Developer name:
            <input type="text" name="new_name" value="<%= dev.name %>" required/>
        </label>
        <button type="submit">Submit</button>
    </form>
</div>